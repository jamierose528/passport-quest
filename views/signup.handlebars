<div class="container" style="font-family: Fuzzy Bubbles, cursive; color: Black">
    <section class="card" style="width: 600px; height: auto;">
			<h3 id="title" style="margin-top: 20px">Sign Up</h3>
			<form class= "signup-form">
				<div class="form-group">
						<p>
								<label for="username"> UserName:*</label><input type="text" id="username-signup" name="userName" style="color: #000000 !important"></input>
						</p>
						<p>
								<label for="email">Email:*</label><input type="text" id="email-signup" name="email" style="color: #000000 !important"></input>
						</p>
						<p>
								<label for="password"> Password:* </label><input type="text" id="password-signup" name="password" style="color: #000000 !important"></input>
						</p>
						<p>
								<label for="firstName"> First Name:* </label><input type="text" id="first_name-signup" name="firstName" style="color: #000000 !important"></input>
						</p>
						<p>
								<label for="middleName"> Middle Name: </label><input type="text" id="middleName" name="middleName" style="color: #000000 !important"></input>
						</p>
						<p>
								<label for="lastName"> Last Name:* </label><input type="text" id="last_name-signup" name="lastName" style="color: #000000 !important"></input>
						</p>
						<p>
								<label for="birthday"> Birthday:* </label><input type="text" id="datepicker" style="color: #000000 !important"></p>
						<p>
								<label for="nationality"> Nationality:* </label><input type="text" id="nationality-signup" name="nationality" style="color: #000000 !important"></input>
						</p>
						<p>
								<label for="homeCountry"> Home Country: </label><input type="text" id="home_country-signup" name="homeCountry" style="color: #000000 !important"></input>
						</p>
						<p><label for="profile_picture"> Profite Picture:*</label></p>			
					<input type="file" id="img" name="img" accept="image/*" style="float: left;">
					<ul class="actions">
					<input type="submit" value="submit" class="button primary">
	</ul>
	</section>
</div>



<script>
	$(function() {
			$("#datepicker").datepicker({
					changeMonth: true,
					changeYear: true
			});
});
</script>


<script src="https://upload-widget.cloudinary.com/global/all.js" type="text/javascript"></script> 

<script src="./js/signup.js "></script>


